                seoText}}/{{escapeUrl seoText}}{{/if}}.webp?w=3200&$badgev8$&{{roundelConfig roundel}} 2x"
                media="(min-width: 1280px)"
                type="image/webp">
        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if seoText}}/{{escapeUrl
                seoText}}{{/if}}?w=1600&$badgev8$&{{roundelConfig
                roundel}} 1x, //{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if
                seoText}}/{{escapeUrl seoText}}{{/if}}?w=3200&$badgev8$&{{roundelConfig roundel}} 2x"
                media="(min-width: 1280px)">

        <source srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if seoText}}/{{escapeUrl
                seoText}}{{/if}}.webp?w=1280&$badgev8$&{{roundelConfig
